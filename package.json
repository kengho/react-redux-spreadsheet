    "css-loader": "^0.28.0",
    "dialog-polyfill": "^0.4.7",
    "domready": "^1.0.8",
    "excel-column-name": "^1.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "immutable": "^3.8.1",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "node-sass": "^4.5.2",
    "null-loader": "^0.1.1",
